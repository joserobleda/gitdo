{
    "name": "GitDo Chrome Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "default_locale": "en",
    "description": "Combine assignee: and preview-requested: Github search filters into a single one \"gitdo:\"",
    "icons": {
        "256":"icon/256.png",
        "128":"icon/128.png",
        "48": "icon/48.png"
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "js": ["app.js"],
            "run_at": "document_end"
        }
    ]
}
